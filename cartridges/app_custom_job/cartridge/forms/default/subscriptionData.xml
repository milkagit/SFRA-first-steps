<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19" secure="false">
	<group formid="storedFormDetails">
		<field
			formid="firstName"
			label="label.input.firstName.subscriptionData"
			mandatory="true"
			max-length="50"
			missing-error="error.message.required"
			range-error="error.message.50orless"
			type="string" />

		<field
			formid="lastName"
			label="label.input.lastName.subscriptionData"
			mandatory="true"
			max-length="50"
			missing-error="error.message.required"
			range-error="error.message.50orless"
			type="string" />

		<field
			formid="email"
			label="label.input.email.subscriptionData"
			mandatory="true"
			max-length="50"
			missing-error="error.message.required"
			parse-error="error.message.parse.email.subscriptionData.form"
			range-error="error.message.50orless"
			regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$"
			value-error="ValueErrorText"
			type="string"/>

		<field
			formid="gender"
			label="label.input.gender.subscriptionData"
			mandatory="false"
			max-length="50"
			missing-error="error.message.required"
			parse-error="error.message.parse.email.subscriptionData.form"
			range-error="error.message.50orless"
			regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$"
			value-error="ValueErrorText"
			type="string"/>
    </group>
</form>
