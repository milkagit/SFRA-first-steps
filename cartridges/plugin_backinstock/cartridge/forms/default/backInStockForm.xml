<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19" >

			<field
				formid="phoneNumbers"
				label="label.input.phoneNumbers.backInStockForm"
				mandatory="true"
				regexp="^\+(?:[0-9] ?){6,14}[0-9]$"
				parse-error="error.message.required.phone"
				missing-error="error.message.required.phone"
				range-error="error.message.range.phone"
				type="string"
			/>

			<action formid="apply" valid-form="true"/>
</form>

